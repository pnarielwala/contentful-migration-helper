{
  "name": "contentful-migration-tool",
  "version": "1.0.0",
  "source": "src/contentful.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "targets": {
    "main": {
      "context": "node"
    }
  },
  "bin": {
    "contentful-migration-tool": "./dist/main.js"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "contentful": "NODE_PATH=./src ts-node src/contentful.ts",
    "test": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{scripts,src}/**/*": "prettier --write"
  },
  "dependencies": {
    "commander": "5.1.0",
    "contentful-export": "7.12.26",
    "contentful-import": "8.1.17",
    "contentful-management": "8.2.0",
    "contentful-migration": "4.8.1",
    "dotenv": "8.2.0",
    "inquirer": "7.3.2",
    "lilconfig": "^2.0.4",
    "param-case": "3.0.4",
    "typescript": "4.6.2",
    "validate": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@parcel/packager-ts": "2.3.2",
    "@parcel/transformer-typescript-types": "2.3.2",
    "@types/inquirer": "6.5.0",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.13",
    "babel-jest": "27.5.1",
    "contentful-cli": "1.4.34",
    "env-cmd": "10.1.0",
    "husky": "4.3.0",
    "jest": "27.5.1",
    "lint-staged": "10.4.2",
    "parcel": "^2.3.2",
    "prettier": "2.1.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.6.0"
  }
}
