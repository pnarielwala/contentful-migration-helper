{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2018", "esnext.asynciterable", "DOM"],
    "sourceMap": false,
    "baseUrl": "src/",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
